    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>org.netbeans.contrib</groupId>
        <artifactId>contrib-parent</artifactId>
        <version>RELEASE90</version>
        <relativePath>..</relativePath>
    </parent>
    <artifactId>org-netbeans-modules-portalpack-servers-opensourcepc</artifactId>
    <version>3.0.5</version>
    <packaging>nbm</packaging>
    <name>OpenPortal Portlet Container</name>
    <description>Plugin to deploy/undeploy portlet application on OpenPortal Portlet Container available on java.net</description>
    <url>http://portalpack.netbeans.org</url>
    <licenses>
        <license>
