                            <explicitValue>0.6</explicitValue>
                        </dependency>
                        <dependency>
                            <id>org.netbeans.api:org-netbeans-modules-editor-indent</id>
                            <type>spec</type>
                            <explicitValue>1.7</explicitValue>
                        </dependency>
                        <dependency>
                            <id>org.netbeans.modules:org-netbeans-modules-java-editor</id>
                            <type>impl</type>
                            <explicitValue/>
                        </dependency>
                        <dependency>
                            <id>org.netbeans.api:org-netbeans-modules-java-source</id>
                            <type>spec</type>
                            <explicitValue>0.67</explicitValue>
