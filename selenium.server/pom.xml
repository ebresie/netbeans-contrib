                    <publicPackages>
                        <publicPackage>org.netbeans.modules.selenium.server</publicPackage>
                        <publicPackage>org.openqa.selenium.server</publicPackage>
                    </publicPackages>
                    <moduleDependencies>
                        <dependency>
                            <id>org.netbeans.api:org-netbeans-libs-junit4</id>
