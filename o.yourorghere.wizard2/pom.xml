                    <requiresRestart>true</requiresRestart>
                    <publicPackages>
                        <publicPackage>org.netbeans.api.wizard</publicPackage>
                        <publicPackage>org.netbeans.modules.wizard2</publicPackage>
                        <publicPackage>org.netbeans.spi.wizard</publicPackage>
                    </publicPackages>
                    <moduleDependencies>
                        <dependency>
                            <id>org.netbeans.api:org-netbeans-api-progress</id>
