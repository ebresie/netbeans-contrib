                        </dependency>
                        <dependency>
                            <id>org.netbeans.api:org-apache-tools-ant-module</id>
                            <type>spec</type>
                            <explicitValue>3</explicitValue>
                        </dependency>
                        <dependency>
                            <id>org.netbeans.api:org-jdesktop-layout</id>
                            <type>spec</type>
                            <explicitValue>1.3.23</explicitValue>
                        </dependency>
                        <dependency>
                            <id>org.netbeans.api:org-netbeans-modules-project-ant</id>
                            <type>spec</type>
                            <explicitValue>1.6.8</explicitValue>
                        </dependency>
                        <dependency>
                            <id>org.netbeans.api:org-netbeans-modules-project-ant</id>
                            <type>spec</type>
                            <explicitValue>1</explicitValue>
                        </dependency>
                        <dependency>
                            <id>org.netbeans.api:org-netbeans-modules-projectapi</id>
                            <type>spec</type>
                            <explicitValue>1.9.22</explicitValue>
                        </dependency>
                        <dependency>
                            <id>org.netbeans.api:org-netbeans-modules-projectuiapi</id>
                            <type>spec</type>
                            <explicitValue>1.13.22.5</explicitValue>
                        </dependency>
                        <dependency>
                            <id>org.netbeans.api:org-openide-actions</id>
                            <type>spec</type>
                            <explicitValue>6</explicitValue>
                        </dependency>
                        <dependency>
                            <id>org.netbeans.api:org-openide-awt</id>
                            <type>spec</type>
                            <explicitValue>6.7.22</explicitValue>
                        </dependency>
                        <dependency>
                            <id>org.netbeans.api:org-openide-dialogs</id>
                            <type>spec</type>
                            <explicitValue>6.5.22</explicitValue>
                        </dependency>
                        <dependency>
                            <id>org.netbeans.api:org-openide-execution</id>
                            <type>spec</type>
                            <explicitValue>9.3</explicitValue>
                        </dependency>
                        <dependency>
                            <id>org.netbeans.api:org-openide-filesystems</id>
                            <type>spec</type>
                            <explicitValue>9.0</explicitValue>
                        </dependency>
                        <dependency>
                            <id>org.netbeans.api:org-openide-loaders</id>
                            <type>spec</type>
                            <explicitValue>5.9.22</explicitValue>
                        </dependency>
                        <dependency>
                            <id>org.netbeans.api:org-openide-nodes</id>
                            <type>spec</type>
                            <explicitValue>6.7.22</explicitValue>
                        </dependency>
                        <dependency>
                            <id>org.netbeans.api:org-openide-util</id>
                            <type>spec</type>
                            <explicitValue>8.0</explicitValue>
                        </dependency>
                    </moduleDependencies>
                </configuration>
            </plugin>
        </plugins>
    </build>
</project>


